package ar.edu.ubp.pdc.fdeschant.examen;

public class RecursosBean {
	public String desc_recurso;
	public String cod_tipo_recurso;
	public String tipo_propietario;
	public String vigente;
	public String nro_leg_personal;
	public String nro_area;
	public String nro_recurso;
	
	public String getDesc_recurso() {
		return desc_recurso;
	}
	public void setDesc_recurso(String desc_recurso) {
		this.desc_recurso = desc_recurso;
	}
	public String getCod_tipo_recurso() {
		return cod_tipo_recurso;
	}
	public void setCod_tipo_recurso(String cod_tipo_recurso) {
		this.cod_tipo_recurso = cod_tipo_recurso;
	}
	public String getTipo_propietario() {
		return tipo_propietario;
	}
	public void setTipo_propietario(String tipo_propietario) {
		this.tipo_propietario = tipo_propietario;
	}
	public String getVigente() {
		return vigente;
	}
	public void setVigente(String vigente) {
		this.vigente = vigente;
	}
	public int getNro_leg_personal() {
		return nro_leg_personal;
	}
	public void setNro_leg_personal(int nro_leg_personal) {
		this.nro_leg_personal = nro_leg_personal;
	}
	public int getNro_area() {
		return nro_area;
	}
	public void setNro_area(int nro_area) {
		this.nro_area = nro_area;
	}
	public int getNro_recurso() {
		return nro_recurso;
	}
	public void setNro_recurso(int nro_recurso) {
		this.nro_recurso = nro_recurso;
	}
	
}
